class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int flag=1;
        int cnt=0;
        int maxi=INT_MIN;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==1){
                cnt++;
                maxi=max(maxi,cnt);
                flag=0;
            }
            else{
                cnt=0;
            }
        }
        if(flag==1){
            return 0;
        }
        return maxi;
    }
};
