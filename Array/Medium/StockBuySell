class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int mini=prices[0];
        int maxi=INT_MIN;
        int sm=0;
        for(int i=0;i<prices.size();i++){
            sm=prices[i]-mini;
            maxi=max(maxi,sm);
            mini=min(mini,prices[i]);
        }
        return maxi;
    }
};
